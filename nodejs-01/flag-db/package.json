{
  "name": "flag-db",
  "version": "1.0.0",
  "description": "You can run docker containers for postgresql",
  "main": "docker-compose.yml",
  "scripts": {
    "docker:build": "docker-compose --file docker-compose.yml up --build --detach",
    "docker:exec": "docker exec -it postgresql-db /bin/bash",
    "docker:exec:root": "docker exec -u root -it postgresql-db /bin/bash"
  },
  "keywords": [
    "docker",
    "postgresql"
  ],
  "author": "mingyuchoo@gmail.com",
  "license": "MIT License"
}
